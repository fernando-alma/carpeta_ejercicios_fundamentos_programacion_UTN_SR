Proceso Ejercicio_3
	Definir a, b, resultado Como Real;
	a <- 78;
	Escribir Sin Saltar "Digite un número: ";
	Leer b;
	
	//Sumamos a y b, y almacenamos resultado
	
	resultado <- (a + b) / (a * b);
	
	Escribir "El resultado es: ",resultado;

FinProceso
